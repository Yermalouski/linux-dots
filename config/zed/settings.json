// To see all of Zed's default settings: ctrl-shift-p
{
  "format_on_save": "off",
  "remove_trailing_whitespace_on_save": true,
  "disable_ai": true,
  "buffer_font_family": "JetBrainsMono Nerd Font",
  "ui_font_family": "Ubuntu",
  "base_keymap": "VSCode",
  "icon_theme": "Zed (Default)",
  "agent": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-latest"
    }
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "telemetry": {
    "metrics": false,
    "diagnostics": false
  },
  "vim_mode": false,
  "ui_font_size": 10.0,
  "buffer_font_size": 10.0,
  "theme": {
    "mode": "dark",
    "light": "One Light",
    "dark": "Gruber Darker - Minimal"
  }
}
